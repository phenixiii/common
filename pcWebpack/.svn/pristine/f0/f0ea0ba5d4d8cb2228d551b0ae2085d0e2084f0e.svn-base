}

.d-display-block{
		display: block;
}



/*多屏幕*/
@media (min-width:768px) {
	body{
			font-size: 1.2rem!important;
	}
	
	.d-rectangle-item{
		width: 20%;
	}
}

@media (min-width:992px) {
	body{
			font-size: 1.2rem!important;
	}
	
	.d-rectangle-item{
		width: 20%;
	}
}

@media (min-width:1200px) {
	body,html,btn,*{
			font-size: 13px;
	} 
	
	.d-rectangle-item{
		width: 14.28%;
	}
	
	.d-shipOver-boxTotal .d-title{
		margin: 0rem 0 !important;
	}
}
 
@media (min-width:1900px) {
	body,html,btn,*{
			font-size: 16px;
	} 
	
	.d-shipOver-boxTotal .d-title{
		margin: 0.95rem 0 !important;
	}
	
	#YearTallyTeuOne .t_num i{
		
		margin: 0 0.4rem;
	}
	
	#YearTallyTeuOne .d-serial {
		margin: 0 0.4rem;
	}
	
	.d-rectangle-item{
		width: 12.5%;
	}
	
	.d-osecend .d-num{
		font-size: 3rem;
	}
	
	/* 码头总览大屏 */
	.d-daping-boxnumber{
		text-align: center;
	}
	
	.d-daping-boxnumber h3 {
		font-size: 30px;
	}
	
	.mapwrap {
    width: 130% !important;
    height: 100%;
    position: absolute;
    left: -18%;
    top: -19%;
	}
	
	.d-warningBody .el-table .cell{
		font-size: 15px;
	}
	
	.d-shipOver-date .d-clocktime{
		font-size: 5rem !important;
	}
		
	.d-shipOver-date .d-wericon .iconfont{
		font-size: 5rem !important;
	}
	
	.d-shipOver-date .d-wealight,.d-shipOver-date .d-clockdate{
		font-size: 2rem !important;
	}
	
	.d-echart-legend ul li{
		padding: 0.5rem 0;
	}
	
	.d-shipOver-boxTotal .d-item strong.bigfontNum{
		font-size: 2.4rem !important;
	}
	
	.d-shipOver-boxTotal .d-item strong{
		  font-size: 1.4rem !important;
	}
	
	/*.d-shipOver-boxTotal .d-totalYear .d-title{
			height: 60% !important;
	}*/
	
	.d-shipOver-boxTotal .d-totalYear .d-planYear{
			height: 30% !important;
	}
	
	.d-shipOver-boxTotal .d-title{
		  font-size: 1.8rem !important;
	}
	
	.d-shipOver-boxTotal .d-box-hd strong{
			font-size: 1.8rem !important;
	}
	
	.d-echart-legend ul li{
		  padding: 0.5rem 0 !important;
	}
	
	
	/*现场监控*/
	.d-siteMonitor, .d-siteMonitor .port-table li, .d-siteMonitor .port-infomation,
	.d-siteMonitor .bosstext,.d-port-head ul li{
			font-size: 24px;
	}
	
	.d-siteMonitor .d-siteTab{
			height: 36px;
	}
}