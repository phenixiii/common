  text-shadow: 0 1px 0 #282828, 0 2px 0 #1e1e1e, 0 3px 0 #141414, 0 4px 0 #0a0a0a, 0 5px 0 #000, 0 0 10px rgba(0, 0, 0, 0.8);
  -webkit-transform-origin: 0 2rem 0;
  -ms-transform-origin: 0 2rem 0;
      transform-origin: 0 2rem 0;
  width: 100%;
}
.count span:before {
  border-bottom: 1px solid #000;
  content: "";
  left: 0;
  position: absolute;
  width: 100%;
}
.count span:after {
  -webkit-box-shadow: inset 0 0 60px rgba(0, 0, 0, 0.35);
          box-shadow: inset 0 0 60px rgba(0, 0, 0, 0.35);
  content: "";
  height: 100%;
  left: 0;
  position: absolute;
  top: 0;
  width: 100%;
}
