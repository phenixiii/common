				<div class="d-newsArea">
					<div class="section-newsBg">
						<el-row type="flex" align="middle" justify="center" class="wow fadeInUp" data-wow-duration="1s" data-wow-delay=".2s">
							<div class="d-section-header">
								<div class="d-tit"><span>新闻公告</span></div>
							</div>
						</el-row>

						<div class="d-section">
							<el-row>
								<el-col v-for="(x,index) in newsLists" :xs="24" :sm="24" :md="12" :lg="12" :xl="12" class="d-item-margin wow fadeInUp d-height-150"
								 data-wow-duration="1s" data-wow-delay=".5s">
									<el-row :gutter="20">
										<el-col :span="6">
											<el-row type="flex" align="middle" justify="center">
												<div class="d-photo">
													<a target="_blank" @click="openDetail(x)">
														<div v-if="x.CoverImg==null||x.CoverImg==''">
															<img src="Images/index/img_new20181102.jpg" class="" style="height: 110px; width: 110px;" />
														</div>
														<div v-else>
															<img v-bind:src="x.CoverImg" class="" style="height: 110px; width: 110px;" />
														</div>
													</a>
												</div>
											</el-row>
										</el-col>
										<el-col :span="16" class="media-body">
											<a @click="openDetail(x)" target="_blank" class="media-heading d-cursor-pointer">{{x.Title}}</a>
											<p>{{x.Summary}}</p>
										</el-col>
										<el-col :span="2"></el-col>
									</el-row>
								</el-col>
							</el-row>
						</div>
					</div>
				</div>
			</el-main>
 
			<div id="wrapper">
				<a href="javascript:void(0)" class="toTop" id="toTop" title="返回顶部" @click="goTop()"><em class="icon-top"></em></a>
			</div>
		</div>


	</body>
